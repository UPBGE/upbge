#include "stdcycles.h"

shader node_separate_rgb(color Image = 0.8,
                         output float R = 0.0,
                         output float G = 0.0,
                         output float B = 0.0)
{
  R = Image[0];
  G = Image[1];
  B = Image[2];
}
