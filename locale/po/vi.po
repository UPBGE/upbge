msgid "Pinned"
msgstr "Ghim Xuống"


msgctxt "Mesh"
msgid "Length 3D"
msgstr "Chiều Dài 3D"


msgctxt "Mesh"
msgid "Area 3D"
msgstr "Diện Tích 3D"


msgctxt "Mesh"
msgid "Object"
msgstr "Đối Tượng"


